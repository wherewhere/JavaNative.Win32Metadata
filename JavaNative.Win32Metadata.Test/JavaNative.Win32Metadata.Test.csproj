<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <CetCompat>false</CetCompat>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <OutputType>Exe</OutputType>
    <TargetFramework>net10.0-windows</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Windows.CsWin32" Version="0.3.269" PrivateAssets="all">
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <AppLocalAllowedLibraries Include="jvm" />
    <ProjectionDocs Include="..\JavaNative.Win32Metadata\bin\*.msgpack" />
    <ProjectionMetadataWinmd Include="..\JavaNative.Win32Metadata\bin\*.winmd" />
  </ItemGroup>
</Project>
